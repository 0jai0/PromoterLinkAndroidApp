{
  "expo": {
    "name": "PromoterLink",
    "slug": "PromoterLink",
    "scheme": "promoterlink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logov3.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logov4.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logov4.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.promoterLink.version_1",
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "promoterlink",
              "host": "*",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "promoterlink.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "www.promoterlink.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/logov3.png",
      "bundler": "metro"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "json",
        "ts",
        "tsx",
        "cjs"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "c0d559d1-bbd2-4b0b-beb7-2b98cad69d93"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ]
    ]
  }
}
